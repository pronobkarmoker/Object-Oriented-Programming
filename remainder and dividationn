package study;
public class java1 {
	public static void main(String[] args) {
			int firstNum = 20;
			int secondNum =3;
			System.out.println("this is the dividation of the two integers:" +(firstNum/ secondNum));
			System.out.println("this is the reminder of the two integers:" +(firstNum-secondNum*(firstNum/ secondNum)));
			}	
	}
